{
  "name": "template.ramadoka",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn dep && tsc",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "fix": "yarn lint -p tsconfig.json -c tslint.fix.json --fix",
    "circular": "madge --circular ./dist",
    "graph": "madge --image graph.png ./dist",
    "_deploy": "rm -rf ./dist && yarn fix && tsc && serverless deploy",
    "dev": "node --inspect=5858 -r ts-node/register ./src/index.dev.ts",
    "dev:watch": "nodemon",
    "deploy": "yarn _deploy --config serverless/production.yml",
    "devdeploy": "yarn _deploy --config serverless/development.yml"
  },
  "dependencies": {
    "@cryptoket/redis-proxy": "^1.0.8",
    "@cryptoket/ts-maybe": "^1.0.17",
    "@cryptoket/ts-memoize": "^1.0.10",
    "@cryptoket/ts-promise-helper": "^1.0.25",
    "@hapi/joi": "^17.1.0",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hapi-joi-to-swagger": "^3.2.0",
    "sha256": "^0.2.0",
    "typescript": "3.8.1-rc",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.11",
    "@types/jest": "^24.0.25",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.28",
    "@types/redis": "^2.8.16",
    "@types/sha256": "^0.2.0",
    "@types/uuid": "^3.4.7",
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "madge": "^3.4.4",
    "md5": "^2.2.1",
    "serverless-domain-manager": "^3.3.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1"
  }
}
